    param.turnType="FlyOver" GimbalPayloadId="102"/>
    <Service Type="WaypointPlanManagerService" VehicleID="3" NumberWaypointsToServe="512" NumberWaypointsOverlap="5"
    param.turnType="FlyOver" GimbalPayloadId="103"/>
    <Service Type="WaypointPlanManagerService" VehicleID="4" NumberWaypointsToServe="512" NumberWaypointsOverlap="5"
    param.turnType="FlyOver" GimbalPayloadId="104"/>

    <Service Type="SendMessagesService" PathToMessageFiles="../MessagesToSend/">

