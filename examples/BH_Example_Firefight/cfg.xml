    <Service Type="MessageLoggerDataService" LogFileMessageCountLimit="1000" FilesPerSubDirectory="1000">
        <LogMessage MessageType="uxas" NumberMessagesToSkip="0"/>
        <LogMessage MessageType="afrl" NumberMessagesToSkip="0"/>
        
    </Service>
</UxAS>


